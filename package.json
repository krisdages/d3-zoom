{
  "name": "@krisdages/d3-zoom",
  "version": "3.0.0",
  "description": "Pan and zoom SVG, HTML or Canvas using mouse or touch input.",
  "homepage": "https://d3js.org/d3-zoom/",
  "repository": {
    "type": "git",
    "url": "https://github.com/d3/d3-zoom.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "zoom",
    "behavior",
    "interaction"
  ],
  "license": "ISC",
  "author": {
    "name": "Mike Bostock",
    "url": "https://bost.ocks.org/mike"
  },
  "type": "module",
  "files": [
    "dist",
    "src/**/*.js"
  ],
  "module": "src/index.js",
  "main": "dist/d3-zoom.min.cjs",
  "jsdelivr": "dist/d3-zoom.min.js",
  "unpkg": "dist/d3-zoom.min.js",
  "exports": {
    "umd": "./dist/d3-zoom.min.js",
    "require": "./dist/d3-zoom.min.cjs",
    "default": "./src/index.js"
  },
  "dependencies": {
    "d3-dispatch": "npm:@krisdages/d3-dispatch@<4",
    "d3-drag": "npm:@krisdages/d3-drag@<4",
    "d3-interpolate": "npm:@krisdages/d3-interpolate@<4",
    "d3-selection": "npm:@krisdages/d3-selection@<4",
    "d3-transition": "npm:@krisdages/d3-transition@<4"
  },
  "devDependencies": {
    "eslint": "7",
    "jsdom": "16",
    "mocha": "9",
    "rollup": "2",
    "rollup-plugin-terser": "7"
  },
  "scripts": {
    "test": "mocha 'test/**/*-test.js' && eslint src test",
    "prepublishOnly": "rm -rf dist && yarn test && rollup -c"
  },
  "engines": {
    "node": ">=12"
  }
}
